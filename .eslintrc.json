{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": [
      1,
      {
        // Ignore arguments starting with _
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser"
}
